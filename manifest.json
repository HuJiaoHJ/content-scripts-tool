{
  "manifest_version": 2,
 
  "name": "money! money! money!",
  "description": "money! money! money!",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "money! money! money!",
    "default_popup": "popup.html"
  }
}

